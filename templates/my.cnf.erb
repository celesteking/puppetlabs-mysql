<%= scope.lookupvar('mysql::params::disclaimer') -%>

[client]
port    = <%= port %>
socket    = <%= socket %>

[mysqld_safe]
socket    = <%= socket %>
nice    = 0
<% if log_error == 'syslog' -%>
syslog
<% end -%>

[myisamchk]
key_buffer=32M
sort_buffer=32M
read_buffer=16M
write_buffer=16M

[mysqldump]
quick
quote-names
max_allowed_packet = 16M

[mysql]
[isamchk]
key_buffer    = 16M

!includedir <%= @conf_dir %>
!includedir <%= @conf_dir_local %>
